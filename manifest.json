{
  "manifest_version": 3,
  "name": "Look Up Naver Map",
  "version": "0.1",
  "icons": {
    "16": "images/map_service_icon_16.png", 
    "48": "images/map_service_icon_48.png", 
    "128": "images/map_service_icon_128.png"
  },
  "permissions": ["contextMenus", "activeTab", "scripting"],
  "background": {
    "service_worker": "background.js"
  },
  "web_accessible_resources": [
    {
      "resources": ["images/*", "pkg/*"],
      "matches": ["<all_urls>"]
    }
  ]
}
